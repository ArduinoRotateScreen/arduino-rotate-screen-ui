<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.arnaugarcia.ars.ui.controller.HomeController">
    <Text text="Welcome to Arduino Rotate Screen">
        <font>
            <Font size="20.0"/>
        </font>
    </Text>
    <Text text="Current displays">
        <VBox.margin>
            <Insets top="10.0"/>
        </VBox.margin>
    </Text>
    <HBox fx:id="displays">
        <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </VBox.margin>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="108.0" stroke="BLACK" strokeType="INSIDE"
                   width="192.0">
            <HBox.margin>
                <Insets right="5.0"/>
            </HBox.margin>
        </Rectangle>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="108.0" stroke="BLACK" strokeType="INSIDE"
                   width="192.0">
            <HBox.margin>
                <Insets right="5.0"/>
            </HBox.margin>
        </Rectangle>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="192.0" stroke="BLACK" strokeType="INSIDE"
                   width="108.0"/>
    </HBox>
</VBox>
